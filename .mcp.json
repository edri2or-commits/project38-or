{
  "mcpServers": {
    "railway": {
      "type": "http",
      "url": "${RAILWAY_MCP_BRIDGE_URL:-https://railway-mcp-bridge.up.railway.app}",
      "headers": {
        "Authorization": "Bearer ${MCP_BRIDGE_TOKEN}"
      }
    },
    "claude-gateway": {
      "type": "http",
      "url": "https://or-infra.com/mcp",
      "headers": {
        "Authorization": "Bearer ${MCP_GATEWAY_TOKEN}"
      }
    },
    "google-workspace": {
      "type": "http",
      "url": "https://or-infra.com/workspace/mcp",
      "headers": {
        "Authorization": "Bearer ${WORKSPACE_MCP_BRIDGE_TOKEN}"
      }
    },
    "gcp-bigquery": {
      "type": "http",
      "url": "https://bigquery.googleapis.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GCP_ACCESS_TOKEN}"
      }
    },
    "gcp-compute": {
      "type": "http",
      "url": "https://compute.googleapis.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GCP_ACCESS_TOKEN}"
      }
    },
    "gcp-gke": {
      "type": "http",
      "url": "https://container.googleapis.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GCP_ACCESS_TOKEN}"
      }
    },
    "gcp-storage": {
      "type": "http",
      "url": "https://storage.googleapis.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GCP_ACCESS_TOKEN}"
      }
    },
    "gcp-maps": {
      "type": "http",
      "url": "https://mapstools.googleapis.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GCP_ACCESS_TOKEN}"
      }
    },
    "gcloud": {
      "command": "npx",
      "args": ["-y", "@google-cloud/gcloud-mcp"]
    }
  }
}
